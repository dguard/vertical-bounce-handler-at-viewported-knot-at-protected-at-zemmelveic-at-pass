{
  "name": "dc-vk-auth-api",
  "version": "0.0.1",
  "description": "Nest VK Auth API Postgres repository",
  "license": "MIT",
  "author": "Alexander Serditov",
  "repository": "https://github.com/dguard/dc-vk-auth-api",
  "scripts": {
    "spawn-db": "./src/scripts/spawn-db.sh",
    "typeorm:cli": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli -f ./ormconfig-migrations.ts",
    "migration-generate": "npm run typeorm:cli -- migration:generate -n",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "./src/scripts/start.sh",
    "start:prod": "ts-node -r dotenv/config -r tsconfig-paths/register src/main.ts",
    "start:dev": "nodemon --config nodemon.json",
    "start:dev:db": "./src/scripts/start-db.sh",
    "start:dev:db:seed": "ts-node -r tsconfig-paths/register src/scripts/seed.ts",
    "start:debug": "nest start --debug --watch",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "pretypeorm": "(rm ormconfig.json || :) && ts-node -r tsconfig-paths/register src/scripts/write-type-orm-config.ts",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "typeorm:migration:generate": "npm run typeorm -- migration:generate -n",
    "typeorm:migration:run": "npm run typeorm -- migration:run",
    "nest": "nest"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.72.0",
    "@nestjs/common": "^6.7.2",
    "@nestjs/core": "^6.7.2",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/passport": "^8.0.1",
    "@nestjs/platform-express": "^6.7.2",
    "@nestjs/swagger": "^3.1.0",
    "@nestjs/typeorm": "^6.2.0",
    "@swc-node/register": "^1.4.2",
    "@swc/core": "^1.2.168",
    "@swc/helpers": "^0.3.8",
    "aws-sdk": "^2.1116.0",
    "axios": "^0.27.0",
    "bcrypt": "^5.0.1",
    "buffer": "^6.0.3",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.10.1",
    "dotenv": "^8.1.0",
    "form-data": "^4.0.0",
    "lodash": "^4.17.15",
    "morgan": "^1.10.0",
    "nanoid": "^3.3.3",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "pg": "^7.12.1",
    "reflect-metadata": "^0.1.13",
    "regenerator-runtime": "^0.13.9",
    "rimraf": "^3.0.0",
    "rxjs": "^6.5.3",
    "swagger-ui-express": "^4.1.2",
    "typeorm": "^0.2.19",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^6.9.0",
    "@nestjs/schematics": "^6.7.0",
    "@nestjs/testing": "^6.7.1",
    "@types/express": "^4.17.1",
    "@types/jest": "^24.0.18",
    "@types/lodash": "^4.14.142",
    "@types/multer": "^1.4.7",
    "@types/node": "^12.7.5",
    "@types/supertest": "^2.0.8",
    "jest": "^24.9.0",
    "nodemon": "^1.19.3",
    "prettier": "^1.18.2",
    "supertest": "^4.0.2",
    "ts-jest": "^24.1.0",
    "ts-loader": "^6.1.1",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^3.14.1",
    "tslint": "^5.20.0",
    "typescript": "^4.6.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  }
}
